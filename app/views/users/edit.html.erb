<h1> Edit User Information Below </h1>
<%=form_for User.new do |f| %>
    <%= f.label :name %><br>
    <%= f.text_field :name %><br><br>

    <%= f.label :email %><br>
    <%= f.text_field :email %><br><br>

    <%= f.label :password %><br>
    <%= f.password_field :password %><br><br>
  
  <%= f.submit "Submit New Info" %>
  <% end %> 